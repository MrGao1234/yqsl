<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zgjt.yqsl.mapper.GoodsMsgValueMapper">

    <insert id="insertGoodsMsgValue">
        insert into goods_msg_value(goods_msg_key_id, value)
        values
        <foreach collection="list" item="goodsMsgValue" separator=",">
            (#{goodsMsgValue.goodsMsgKeyId}, #{goodsMsgValue.value})
        </foreach>
    </insert>
</mapper>
